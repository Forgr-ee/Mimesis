{
  "name": "mimesis",
  "version": "2.5.0",
  "license": "AGPL-3.0-only",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && cap copy",
    "resources": "cordova-res --skip-config --copy --icon-background-source '#e67f3c'",
    "sync": "cap sync",
    "preview": "vite preview",
    "serve": "vite preview",
    "coverage": "vitest run --coverage",
    "test": "vitest --ui",
    "test:run": "vitest run",
    "seed": "ts-node --project ./node.tsconfig.json seed_db/import_db.ts",
    "types": "node ./scripts/getTypes.mjs",
    "dev-types": "BRANCH=development node ./scripts/getTypes.mjs",
    "lint:markup": "vue-tsc --noEmit",
    "lint:pretty": "prettier src/**/*.{css,vue}",
    "lint:script": "eslint --ext .ts,vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@capacitor-community/keep-awake": "2.1.1",
    "@capacitor/app": "^4.0.0",
    "@capacitor/core": "^4.0.0",
    "@capacitor/device": "^4.0.0",
    "@capacitor/haptics": "^4.0.0",
    "@capacitor/keyboard": "^4.0.0",
    "@capacitor/preferences": "^4.0.1",
    "@capacitor/splash-screen": "^4.0.0",
    "@capacitor/status-bar": "^4.0.0",
    "@capgo/capacitor-purchases": "^1.3.15",
    "@capgo/capacitor-updater": "4.0.0-alpha.52",
    "@faker-js/faker": "7.4.0",
    "@forgr/native-audio": "^3.1.1",
    "@headlessui/vue": "1.6.7",
    "@heroicons/vue": "1.0.6",
    "@intlify/vite-plugin-vue-i18n": "^6.0.1",
    "@ionic/vue": "6.2.4",
    "@ionic/vue-router": "6.2.4",
    "@supabase/supabase-js": "1.35.7",
    "canvas-confetti": "1.5.1",
    "capacitor-rate-app": "^3.0.0",
    "dotenv": "^16.0.3",
    "firebase": "8.10.1",
    "pinia": "2.0.21",
    "plausible-tracker": "^0.3.8",
    "tslib": "^2.4.0",
    "uuid": "^8.3.2",
    "vue": "3.2.37",
    "vue-i18n": "9.2.2",
    "vue-router": "4.1.5",
    "vue-timer-hook": "1.0.11",
    "windicss": "3.5.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.1",
    "@capacitor/android": "^4.0.0",
    "@capacitor/cli": "^4.0.0",
    "@capacitor/ios": "^4.0.0",
    "@types/canvas-confetti": "^1.4.3",
    "@types/jest": "28.1.8",
    "@types/uuid": "8.3.4",
    "@vitejs/plugin-vue": "3.0.3",
    "@vitest/ui": "^0.22.1",
    "@vue/test-utils": "2.0.2",
    "csvtojson": "^2.0.10",
    "eslint": "8.23.0",
    "firebase-admin": "^11.0.1",
    "husky": "8.0.1",
    "jest": "29.0.1",
    "jest-mock-random": "^1.1.1",
    "jsdom": "^20.0.0",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.8.2",
    "vite": "3.0.9",
    "vite-plugin-environment": "1.1.2",
    "vite-plugin-windicss": "1.8.7",
    "vitest": "0.22.1",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "0.40.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint --fix",
    "*": "prettier -w -u"
  }
}
